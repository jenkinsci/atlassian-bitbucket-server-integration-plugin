<?xml version="1.0" encoding="UTF-8"?>
<ruleset xmlns="http://mojo.codehaus.org/versions-maven-plugin/rule/2.0.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://mojo.codehaus.org/versions-maven-plugin/rule/2.0.0 https://mojo.codehaus.org/versions-maven-plugin/xsd/rule-2.0.0.xsd">

    <rules>
        <rule groupId="com.atlassian.bitbucket.server" artifactId="*">
            <ignoreVersions>
            <ignoreVersion type="regex">.*-[a-zA-Z0-9]+</ignoreVersion>
            </ignoreVersions>
        </rule>
    </rules>
</ruleset>